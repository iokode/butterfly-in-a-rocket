---
import Layout from "../layouts/Layout.astro";
import {Markdown} from 'astro-remote';
import {getEntry} from "astro:content";

const entry = await getEntry('essentials', 'ival')!;
---

<Layout title="IOKode Visual Asset License (IVAL)" description="The IOKode Visual Asset License (IVAL) defines the terms and conditions for using visual assets, graphics, and design elements from the IOKode project.">
    <Markdown content={entry.data.body}/>
</Layout>